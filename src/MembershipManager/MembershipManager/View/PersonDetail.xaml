<Window x:Class="MembershipManager.View.PersonDetail"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MembershipManager.Utils.UserControls"
        mc:Ignorable="d"
        Title="PersonDetail" Height="450" Width="800">
    <StackPanel>
        <local:EntryH x:Name="EntryAvs"    Title="No AVS"/>
        <local:EntryH x:Name="EntryName"   Title="Nom"/>
        <local:EntryH x:Name="EntryPrenom" Title="Prenom"/>
        <local:EntryH x:Name="EntryEmail"  Title="Email"/>
        <local:EntryH x:Name="EntryTel"    Title="Tel"/>
        <local:EntryH x:Name="EntryAdr"    Title="Adresse"/>

    </StackPanel>
</Window>
